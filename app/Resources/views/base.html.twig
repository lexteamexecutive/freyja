<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{% block title %}LexTeam Executive{% endblock %}</title>

    {% block plugins_css %}
      {% stylesheets
        'plugins/datatables/jquery.dataTables.min.css'
        'plugins/datatables/buttons.bootstrap.min.css'
        'plugins/datatables/fixedHeader.bootstrap.min.css'
        'plugins/datatables/responsive.bootstrap.min.css'
        'plugins/toastr/toastr.min.css'
        'plugins/select2/dist/css/select2.css'
        'plugins/select2/dist/css/select2-bootstrap.css'
        'plugins/bootstrap-sweetalert/sweet-alert.css'
        'plugins/switchery/switchery.min.css'
        filter='cssrewrite'
        output='css/compiled/plugins.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
      {% endstylesheets %}

    {% stylesheets
      'css/bootstrap.min.css'
      'css/core.css'
      'css/components.css'
      'css/icons.css'
      'css/pages.css'
      'css/menu.css'
      'css/responsive.css'
      'css/main.css'
      filter='cssrewrite'
      output='css/compiled/app.css' %}
      <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% endblock %}

    {% stylesheets
      'css/bootstrap.min.css'
      filter='cssrewrite'
      output='css/compiled/app.css' %}
      <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    {% javascripts
      'js/libs/modernizr.min.js'
      output='js/compiled/modernizr.js' %}
      <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
</head>
<body>
{% block navbar %}
{% include "navbar.html.twig" %}
{% endblock %}

  <div class="wrapper">
    <div class="container-fluid">
      {% block page_title %}
        <!-- Page-Title -->
        <div class="row">
            PAGE TITLE
        </div>
      {% endblock %}

      {% block body %}{% endblock %}

      {% block footer %}
        <!-- Footer -->
        <footer class="footer text-right">
          FOOTER
        </footer>
        <!-- End Footer -->
      {% endblock %}
    </div>
    <!-- end container -->

      {% block right_sidebar %}
        <!-- Right Sidebar -->
        <div class="side-bar right-bar">
            SIDEBAR RIGHT
        </div>
        <!-- /Right-bar -->
      {% endblock %}
  </div>

  {% block javascripts %}
    {% javascripts
        'js/libs/jquery.min.js'
        'js/libs/bootstrap.min.js'
        'js/libs/detect.js'
        'js/libs/fastclick.js'
        'js/libs/waves.js'
        'js/libs/wow.min.js'
        output='js/compiled/app.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  {% endblock %}

  {% block plugins_js %}
    {% javascripts
        'plugins/bootstrap-sweetalert/sweet-alert.min.js'
        'plugins/datatables/jquery.dataTables.min.js'
        'plugins/datatables/dataTables.bootstrap.js'
        'plugins/datatables/dataTables.buttons.min.js'
        'plugins/datatables/buttons.bootstrap.min.js'
        'plugins/toastr/toastr.min.js'
        'plugins/parsleyjs/dist/parsley.min.js'
        'plugins/select2/dist/js/select2.min.js'
        'plugins/switchery/switchery.min.js'
      output='js/compiled/plugins.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  {% endblock %}

  {% block jqueries %}
    {% javascripts
        'js/libs/jquery.core.js'
        'js/libs/jquery.app.js'
      output='js/compiled/jqueries.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  {% endblock %}

  {% block javascript_main %}
    {% javascripts
        'js/main.js'
        'js/users.js'
        output='js/compiled/main.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  {% endblock %}

  {#
      Init Declarations Plugins JS
  #}
  {% block toastr %}
  {% endblock %}

  {% block parsley %}
  {% endblock %}

  {% block select2 %}
  {% endblock %}
  </body>
</html>
